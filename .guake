[general]
abbreviate-tab-names=false
compat-delete='delete-sequence'
default-shell='/usr/bin/tmux'
display-n=0
history-size=1000
infinite-history=true
max-tab-name-length=150
mouse-display=true
open-tab-cwd=true
prompt-on-quit=true
quick-open-command-line='gedit %(file_path)s'
restore-tabs-notify=false
restore-tabs-startup=false
save-tabs-when-changed=true
scroll-keystroke=true
start-at-login=true
start-fullscreen=false
use-default-font=true
use-login-shell=false
use-popup=true
use-scrollbar=true
use-trayicon=false
use-vte-titles=true
window-halignment=2
window-height=100
window-horizontal-displacement=0
window-losefocus=false
window-refocus=false
window-tabbar=false
window-vertical-displacement=0
window-walignment=100
window-width=49

[style/background]
transparency=100

[style/font]
allow-bold=true
palette='#555557575353:#555557575353:#555557575353:#f0f0dfdfafaf:#505060607070:#dcdc8c8cc3c3:#8c8cd0d0d3d3:#dcdcdcdccccc:#707090908080:#dcdca3a3a3a3:#c3c3bfbf9f9f:#e0e0cfcf9f9f:#9494bfbff3f3:#ecec9393d3d3:#9393e0e0e3e3:#ffffffffffff:#dcdcdcdccccc:#2e2e34343636'
